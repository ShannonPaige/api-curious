<div>
  <% if current_user %>
    <p>Hello, <%= current_user.name %></p>
    <%= link_to "Logout", logout_path %>
    <%= render partial: 'shared/profile' %>
    <%= render partial: 'shared/tweet_deck' %>
    <%= render partial: 'shared/twitter_feed' %>
  <% else %>
    <div class="jumbotron">
      <%= link_to "#{image_tag("http://i.imgur.com/7Jq3pi2.jpg")}".html_safe, login_path %>
    </div>
  <% end %>
</div>
